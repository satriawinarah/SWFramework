<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SWEditText">
        <attr name="title" format="string"/>
        <attr name="description" format="string"/>
        <attr name="placeholder" format="string"/>
        <attr name="warning" format="string"/>
        <attr name="error" format="string"/>
        <attr name="android:inputType"/>
    </declare-styleable>

    <attr name="CustomActionBar" format="enum">
        <enum name="default_action_bar" value="0"/>
        <enum name="search_action_bar" value="1"/>
        <enum name="save_action_bar" value="2"/>
    </attr>

    <declare-styleable name="CustomActionBar">
        <attr name="action_bar_type"/>
    </declare-styleable>

</resources>